<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.datapoliceukapp" version="1.2.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>data.police.uk App</name>
    <description>data.police.uk App</description>
    <author email="ashoornamrood@gmail.com" href="https://github.com/namroodinc/">Ashoor Namrood</author>
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
